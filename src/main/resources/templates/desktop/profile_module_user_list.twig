{% extends "profile_module" %}
{% block moduleContent %}
<table width="100%" cellpadding="0" cellspacing="0" border="0">
	<colgroup><col width="48"/><col width="*"/></colgroup>
{% for admin in users %}
	{% if loop.index<5 %}
	<tr class="oneMemberRow">
		<td valign="top"><a href="{{ admin.user.profileURL }}"{{ profileRel(admin.user) }}>{{ admin.user | pictureForAvatar('a', 40) }}</a></td>
		<td class="nameAndInfo">
			<div><a href="{{ admin.user.profileURL }}"{{ profile.rel(admin.user) }}>{{ admin.user | name }}</a></div>
			{% if admin.title is not empty %}
				<div class="info">{{ admin.title }}</div>
			{% endif %}
		</td>
	</tr>
	{% endif %}
{% endfor %}
</table>
{% endblock %}